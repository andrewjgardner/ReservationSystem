@model ReservationSystem.Models.Reservation.Create

<div class="row m-3">
    <div class="col-md-5 offset-md-1">
        <partial name="_Calendar" />
    </div>
    <div class="col-md-5">
        <form asp-action=Create>
            <partial name="_ReservationFormPartial" for="ReservationForm" />
            <div>
                <button type="submit" class="btn btn-primary">Submit</button>
            </div>
            <input type="hidden" asp-for=SittingId />
        </form>
    </div>
</div>
@section Scripts{
<script src="/js/calendar.js"></script>
<script src="~/js/calendarPartial.js"></script>
}